    <jsScripts>      <jsScript>        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>&#47;&#47;Script here

&#47;&#47;regex = &#47;(\d\d)&#47;g;
&#47;&#47;nro_ccz_aux = regex.exec(ZONA_LEGAL);
&#47;&#47;var NRO_CCZ = parseInt(nro_ccz_aux[0]);
&#47;&#47;Alert(NRO_CCZ);

var nro_ccz_aux = substr(trim(ZONA_LEGAL),3);
var NRO_CCZ = nro_ccz_aux[0] == &quot;0&quot; ? parseInt(nro_ccz_aux[1]) : parseInt(nro_ccz_aux);  

&#47;&#47;Alert(ZONA_LEGAL + &quot; - &quot; + substr(trim(ZONA_LEGAL),3) + &quot; - &quot; + NRO_CCZ);

var COD_MUN = &apos;SD&apos;;

switch (NRO_CCZ) {
	case 14:
	case 17:
	case 18:
			COD_MUN = &apos;A&apos;;
			break;
	case 1:
	case 2:
			COD_MUN = &apos;B&apos;;
			break;
	case 3:
	case 15:
	case 16:
			COD_MUN = &apos;C&apos;;
			break;
	case 4: 
	case 5:
			COD_MUN = &apos;CH&apos;;
			break;
	case 10:
	case 11:
			COD_MUN = &apos;D&apos;;
			break;
	case 6:
	case 7:
	case 8: 
			COD_MUN = &apos;E&apos;;
			break;
	case 9:
			COD_MUN = &apos;F&apos;;
			break;
	case 12:
	case 13:
			COD_MUN = &apos;G&apos;;
			break;
	default:;
}
</jsScript_script>
