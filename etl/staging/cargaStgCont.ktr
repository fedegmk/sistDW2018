TURNO_HORARIO = replace(TURNO_HORARIO,&quot;SABADO[S]&quot;,&quot;SAB&quot;);
TURNO_HORARIO = replace(TURNO_HORARIO,&quot;DOMINGO[S]&quot;,&quot;DOM&quot;);
TURNO_HORARIO = replace(TURNO_HORARIO,&quot;,&quot;,&quot;&quot;);
